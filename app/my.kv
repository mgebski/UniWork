			source: 'l.jpeg'
	GridLayout:
		cols:1
		BoxLayout:
			pos_hint: {'x': 0 ,"top":1}
			height:300
			Button:
				size_hint:(None,None)
				text:"Back"
				on_release:
					app.root.current = "third"
					root.manager.transition.direction = "right"
		
		FloatLayout:
			GridLayout:
				pos_hint: {'x': 0 ,"top":1}
				cols:1
				Label:
					text:"Settings"
