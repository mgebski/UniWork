<NinthWindow>:
	name:"ninth"
	
	email:email
	username:username
	password:password
	con_password:con_password
	dob:dob
	
	canvas:
		Rectangle:
			pos: self.pos
			size: self.size
			source: 'r.jpeg'
	
	GridLayout: 	
		cols:1
		pos_hint: {'x': 0.35 ,"y":-0.35}
		
		
		GridLayout:
			cols:1
			size_hint:(None,None)
			
			TextInput:
				id:email
				hint_text: "Email"
				multiline:False
				size_hint: (None, None)
				height: 50
				width: 500
				background_color: (0,0,0,0.5)
				on_text: self.foreground_color = (255,255,255,1)
			TextInput:
				id:username
				hint_text: "Username"
				multiline:False
				size_hint: (None, None)
				height: 50
				width: 500
				background_color: (0,0,0,0.5)
				on_text: self.foreground_color = (255,255,255,1)
			TextInput:
				id:password
				password: True
				hint_text: "Password"
				multiline:False
				size_hint: (None, None)
				height: 50
				width: 500
				background_color: (0,0,0,0.5)
				on_text: self.foreground_color = (255,255,255,1)
			TextInput:
				id:con_password
				password: True
				hint_text: "Confirm Password"
				multiline:False
				size_hint: (None, None)
				height: 50
				width: 500
				background_color: (0,0,0,0.5)
				on_text: self.foreground_color = (255,255,255,1)
			TextInput:
				id:dob
				hint_text: "DOB"
				multiline:False
				size_hint: (None, None)
				height: 50
				width: 500
				background_color: (0,0,0,0.5)
				on_text: self.foreground_color = (255,255,255,1)
			
			BoxLayout:
				size_hint: (None,None)
				height: 50
				width: 500
				spacing: 10
				SmoothButton:
					text:"Complete"
					on_release:
						root.btn()
						app.root.current = "main"
						root.manager.transition.direction = "down"
					back_color:(0.157,0.455,0.752,0.5)